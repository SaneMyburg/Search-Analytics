<h3>Search Analytics:</h3>
<table>
  <thead>
    <tr>
      <th>Search Query</th>
      <th>Count</th>
    </tr>
  </thead>
  <tbody>
    <% @search_queries.each do |query, count| %>
      <tr>
        <td><%= query %></td>
        <td><%= count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
